generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Guild {
  id        String   @id @map("_id") @db.String
  locale    String   @default("en-US")
  caseCount Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
