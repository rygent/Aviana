{
  "name": "@elvia/bot",
  "version": "5.5.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && yarn build:check && yarn build:swc",
    "build:check": "tsc --noEmit",
    "build:swc": "swc ./src --out-dir ./dist --copy-files --strip-leading-paths",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "start": "node --no-warnings ./dist/index.js",
    "register": "node ./dist/interactions/register.js"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gihub.com/Rygent/Elvia.git",
    "directory": "apps/bot"
  },
  "dependencies": {
    "@elvia/database": "workspace:^",
    "@elvia/logger": "workspace:^",
    "@iamtraction/google-translate": "^2.0.1",
    "@prisma/client": "^5.15.0",
    "@rygent/anilist": "^1.4.0",
    "@rygent/spotify": "^1.1.1",
    "@sapphire/discord-utilities": "^3.3.0",
    "@sapphire/time-utilities": "^1.7.12",
    "@sapphire/type": "^2.4.4",
    "@sapphire/utilities": "^3.16.2",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "discord-api-types": "^0.37.89",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "globby": "^14.0.1",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^5.0.7",
    "systeminformation": "^5.22.11",
    "undici": "^6.19.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.1",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^20.14.2",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.4"
  },
  "engines": {
    "node": ">=v20.10.0"
  },
  "packageManager": "yarn@4.3.1"
}
